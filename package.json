    "migration:create": "yarn typeorm migration:create",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/configs/datasource.config.ts",
    "docker-build": "docker build --build-arg NODE_VERSION=$(cat .nvmrc | sed 's/v//') -t api-starter ."
